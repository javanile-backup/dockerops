 * MIT Licensed
 */

"use strict";

var fs = require("fs");
var path = require("path");
var local = path.join(process.cwd(), "node_modules/dockerops/src/cli.js");
var argv = process.argv.slice(2);
var cli = null;

if (!fs.existsSync(local)) {
    cli = require("./src/cli");
} else {
    cli = require(local);
}

cli.run(argv, function (output) {
    console.log(output);
});
